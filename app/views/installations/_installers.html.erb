<h3>Installers</h3>
<div class="panel"> 	

		<table> 
			<tr>
				<th>Number</th> 
				<th>Name</th> 
				<th>Contact</th> 
			</tr>
			
			<% @installers.each do |installer| %>
			    <tr class="<%= cycle("even", "odd") %>">
			    	<td><%= installer.installer_number %></td>
					<td><%= installer.name + ' ' + installer.surname  %></td> 
					<td><%= installer.contact_number %></td>
				</tr>
				
			<% end %> 
		</table>
</div>